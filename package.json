  "devDependencies": {
    "@types/bun": "latest",
    "@types/exceljs": "^1.3.2",
    "@types/inquirer": "^9.0.8"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "dependencies": {
    "@types/fast-levenshtein": "^0.0.4",
    "@types/figlet": "^1.7.0",
    "@xenova/transformers": "^2.17.2",
    "chalk": "^5.4.1",
    "exceljs": "^4.4.0",
    "fast-levenshtein": "^3.0.0",
    "inquirer": "8.2.5",
    "inquirer-file-tree-selection-prompt": "^2.0.5"
  }
