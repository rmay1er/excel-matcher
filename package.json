{
  "name": "excel-matcher",
  "module": "index.ts",
  "type": "module",
  "version": "1.2.0",
  "private": true,
  "scripts": {
    "dev": "bun run index.ts",
    "compile": "bun build --compile --minify-whitespace --minify-syntax --target bun --outfile dist/excel-matcher ./index.ts",
